#uploadModal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='exampleModalLabel', aria-hidden='true')
  .modal-dialog(role='document')
    .modal-content
      .modal-header
        h5#exampleModalLabel.modal-title Modal title
        button.close(type='button', data-dismiss='modal', aria-label='Close')
          span(aria-hidden='true') Ã—
      .modal-body
        input#logs-upload.mr-2.filestyle.col-sm-9(type="file", name="file", data-text="upload log file", data-btnClass="btn-secondary", data-buttonBefore="true", data-placeholder="", data-source=)                                            
				#logs-progress.progress.ml-2.invisible(style="width: 250px;")
				  #logs-progress-bar.progress-bar.progress-bar-striped.progress-bar-animated(role='progressbar', aria-valuenow='0', aria-valuemin='0', aria-valuemax='100', style='width: 0%; height: 35px;')
      .modal-footer
        button.btn.btn-secondary(type='button', data-dismiss='modal') Close
        button.btn.btn-primary(type='button') Save changes

				