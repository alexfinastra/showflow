<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://rp.baml.com/service/FXOrdersV001" xmlns:fml="http://www.fixprotocol.org/FIXML-4-4" xmlns:fxtml="http://rp.baml.com/fxt/FIXML-4-4" xmlns:soap="http://www.w3.org/2003/05/soap-envelope" targetNamespace="http://rp.baml.com/service/FXOrdersV001" elementFormDefault="qualified">
	
	<xs:import namespace="http://www.fixprotocol.org/FIXML-4-4" schemaLocation="FIXML-4.4-Container.xsd"/>
	<xs:import namespace="http://rp.baml.com/fxt/FIXML-4-4" schemaLocation="FXT-FIXML-4-4.xsd"/>
	<xs:complexType name="FXOrderMessage_Type">
		<xs:sequence>
			<xs:element name="FIXML" type="fxtml:FIXML_Type"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FXOrderBaseRequest_Type">
		<xs:complexContent>
			<xs:extension base="tns:FXOrderMessage_Type"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="FXOrderBaseResponse_Type">
		<xs:complexContent>
			<xs:extension base="tns:FXOrderMessage_Type">
				<xs:choice>
					<xs:element ref="fxtml:ExecRpt"/>
					<xs:element ref="fxtml:Rej"/>
				</xs:choice>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="FXOrderInitiation">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tns:FXOrderBaseRequest_Type">
